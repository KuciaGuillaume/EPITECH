#include <unistd.h>

int my_strupcase(char *str)
{
    char mot;
    int cpt = 0;
    int mcpt = 0;

    while (cpt != '\0') {
        if (str[cpt] >= 65 && str[cpt] <= 122) {
            mot[mcpt] = str[cpt] - 32;
            mcpt++;
            cpt++;
        } else {
            mot[mcpt] = str[cpt];
            cpt++;
        }
    }
    return mot;
}

void main(void)
{
    my_strupcase("yj,uk,g::<:;:AgrrduAAATGGFFGHjgbdg");
}
